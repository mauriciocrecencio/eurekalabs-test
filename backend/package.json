{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": "14.7.0"
  },
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "postinstall": "tsc",
    "watch-node": "nodemon dist/index.js",
    "watch-ts": "tsc -w",
    "deploy": "git add . && git commit -m Heroku && git push heroku master",
    "dev": "ts-node-dev --transpile-only --ignore-watch node_modules src/index.ts",
    "typeorm": "ts-node-dev node_modules/typeorm/cli.js"
  },
  "keywords": [
    "example",
    "heroku"
  ],
  "license": "MIT",
  "dependencies": {
    "@types/mocha": "^8.2.1",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mocha": "^8.3.1",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "sqlite3": "^5.0.2",
    "ts-node-dev": "^1.1.6",
    "typeorm": "^0.2.31",
    "typescript": "^4.2.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/request": "^2.48.5",
    "@types/uuid": "^8.3.0",
    "typescript": "^4.2.3"
  }
}
